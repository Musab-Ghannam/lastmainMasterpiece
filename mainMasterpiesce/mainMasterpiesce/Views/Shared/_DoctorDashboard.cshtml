        <div class="container-fluid">

            <div class="row">
                <div class="col-md-5 col-lg-4 col-xl-3 theiaStickySidebar">

                    <!-- Profile Sidebar -->
                    <div class="profile-sidebar">
                        <div class="widget-profile pro-widget-content">
                            <div class="profile-info-widget">
                                <a href="#" class="booking-doc-img">
                                    <img src="~/Content/images/@Model.Item1[0].picdoctor" alt="User Image">
                                </a>

                                <div class="profile-det-info">
                                    <h3>Dr-@Model.Item1[0].doctorName</h3>

                                    <div class="patient-details">
                                        @if (Model.Item1[0].specialization1 != null)
                                        {



                                            <h5 class="mb-0">@Model.Item1[0].specialization1.namespecialization</h5>}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="dashboard-widget">
                            @{

                                string url = Request.Url.AbsoluteUri; // get the absolute URL of the current request
                                string[] segments = url.Split('/'); // split the URL into segments
                                string lastSegment = segments[segments.Length - 1]; }
                            <nav class="dashboard-menu">



                                <ul>
                                    <li class="@(lastSegment == "DoctorDashboard" ? "active" : "")">
                                        <a href="DoctorDashboard">
                                            <i class="fas fa-columns"></i>
                                            <span>Appointments</span>
                                        </a>
                                    </li>





                                    <li class="@(lastSegment == "NotAvailable" ? "active" : "")">
                                        <a href="NotAvailable">
                                            <i class="fas fa-hourglass-start"></i>
                                            <span>Schedule Timings</span>
                                        </a>
                                    </li>
                                    <li class="@((lastSegment == "MyReviews" || lastSegment == "PendingReviews") ? "active" : "")">
                                        <a href="MyReviews">
                                            <i class="fas fa-star"></i>
                                            <span>Reviews</span>
                                        </a>
                                    </li>





                                    <li class="@(lastSegment == "DoctorDashSetting" ? "active" : "")">
                                        <a href="DoctorDashSetting">
                                            <i class="fas fa-user-cog"></i>
                                            <span>Profile Settings</span>
                                        </a>
                                    </li>

                                    <li>
                                        <a href="doctor-change-password.html">
                                            <i class="fas fa-lock"></i>
                                            <span>Change Password</span>
                                        </a>
                                    </li>




                                    <li>
                                        <a href="@Url.Action("LogOfff", "Account")">
                                            <i class="fas fa-sign-out-alt"></i>
                                            <span>Logout</span>
                                        </a>
                                    </li>











                                </ul>



</nav>
                        </div>
                    </div>
                    <!-- /Profile Sidebar -->

                </div>








